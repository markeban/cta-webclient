<h1>Create a new Train Line</h1>

<div class="form">
  <%= form_tag(trains_path) do %>
    <fieldset>
      <span>Train </span><%= label_tag :name %>
      <%= text_field_tag :name %><br>

      <%= label_tag :status %>
      <%= text_field_tag :status %><br>

      <%= submit_tag "Create Train" %>
    </fieldset>
  <% end %>
</div>